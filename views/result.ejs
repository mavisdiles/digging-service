                </div>
            </div>
            <div id="myCarousel-two" class="carousel slide">
                <!-- Wrapper for Slides -->
                <div class="carousel-inner artist-wrapper">
                    <div class="item active">
                        <!-- artist Member One -->
                        <div class="col-xs-12 col-sm-4 col-md-4 artist-member">
                            <img src="img/artist/david.png" alt="artist-member-img1">
                        </div>
                        <div class="col-xs-12 col-sm-8 col-md-8 artist-member-bio">
                            <h3 class="artist-member-name"> <%=data%> </h3>
                            <p class="text-muted-role">Full Stack Developer</p>
